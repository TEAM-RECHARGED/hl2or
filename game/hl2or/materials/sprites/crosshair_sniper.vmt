"UnlitGeneric"
{
	"$basetexture" "sprites/crosshair_sniper"
	"$additive" "1"
	"$vertexcolor" "1"
	"$vertexalpha" "1"
	"$ignorez"     "1"
"Proxies"
{
     "AnimatedTexture"
     {
    "animatedtexturevar" "$basetexture"
    "animatedtextureframenumvar" "$frame"
    "animatedtextureframerate" "50"
     }
}
}
