"UnlitGeneric"
{
	"$basetexture" "effects/ar2/combinemuzzle1"
	"$surfaceprop"	"default"
	"$translucent"	1
	"$vertexcolor" 	1
	"$vertexalpha"	1
	"$additive"	1
"Proxies"
{
     "AnimatedTexture"
     {
    "animatedtexturevar" "$basetexture"
    "animatedtextureframenumvar" "$frame"
    "animatedtextureframerate" "15"
     }
}
}
