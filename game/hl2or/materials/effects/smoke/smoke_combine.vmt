"UnlitGeneric"
{
	// Original shader: BaseTimesVertexColorAlphaBlend
	"$basetexture" "effects/smoke/smoke_combine"
	"$translucent" "1"
	"$vertexcolor" "1"
	"$vertexalpha" "1"
	"$additive"	1
"Proxies"
{
     "AnimatedTexture"
     {
    "animatedtexturevar" "$basetexture"
    "animatedtextureframenumvar" "$frame"
    "animatedtextureframerate" "3"
     }
}